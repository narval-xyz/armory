{
  "name": "aa-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "demos/aa-client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/demos/aa-client",
        "main": "demos/aa-client/src/index.ts",
        "tsConfig": "demos/aa-client/tsconfig.app.json",
        "webpackConfig": "demos/aa-client/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "aa-client:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "aa-client:build:development"
        },
        "production": {
          "buildTarget": "aa-client:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["demos/aa-client/**/*.ts"]
      }
    },
    "test:type": {
      "executor": "nx:run-commands",
      "options": {
        "command": "make aa-client/test/type"
      }
    },
    "test:unit": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "demos/aa-client/jest.unit.ts",
        "verbose": true
      }
    },
    "test:integration": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "demos/aa-client/jest.integration.ts",
        "verbose": true,
        "runInBand": true
      }
    },
    "test:e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "demos/aa-client/jest.e2e.ts",
        "verbose": true,
        "runInBand": true
      }
    }
  },
  "tags": ["type:application"]
}

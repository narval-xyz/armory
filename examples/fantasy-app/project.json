{
  "name": "fantasy-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/fantasy-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/examples/fantasy-app",
        "main": "examples/fantasy-app/src/index.ts",
        "tsConfig": "examples/fantasy-app/tsconfig.app.json",
        "webpackConfig": "examples/fantasy-app/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "prerequisites:build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/examples/fantasy-app",
        "main": "examples/fantasy-app/src/prerequisites.ts",
        "tsConfig": "examples/fantasy-app/tsconfig.app.json",
        "webpackConfig": "examples/fantasy-app/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "prerequisites": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "fantasy-app:prerequisites:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "fantasy-app:prerequisites:build:development"
        },
        "production": {
          "buildTarget": "fantasy-app:prerequisites:build:production"
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "fantasy-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "fantasy-app:build:development"
        },
        "production": {
          "buildTarget": "fantasy-app:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["examples/fantasy-app/**/*.ts"]
      }
    }
  },
  "tags": ["type:application"]
}

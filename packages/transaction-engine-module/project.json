{
  "name": "transaction-engine-module",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/transaction-engine-module/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/transaction-engine-module",
        "tsConfig": "packages/transaction-engine-module/tsconfig.lib.json",
        "packageJson": "packages/transaction-engine-module/package.json",
        "main": "packages/transaction-engine-module/src/index.ts",
        "assets": ["packages/transaction-engine-module/*.md"]
      }
    },
    "lint": {
      "options": {
        "args": ["./**/*.ts", "./package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/transaction-engine-module/jest.config.ts"
      }
    }
  }
}
